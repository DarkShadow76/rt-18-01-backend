# Database Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Cloud Document AI Configuration
GOOGLE_CLOUD_PROJECT_ID=your_project_id_here
GOOGLE_CLOUD_LOCATION=your_location_here
GOOGLE_CLOUD_PROCESSOR_ID=your_processor_id_here
GOOGLE_CLOUD_CLIENT_EMAIL=your_client_email_here
GOOGLE_CLOUD_PRIVATE_KEY=your_private_key_here

# Application Configuration
PORT=4447
FRONTEND_URL=http://localhost:3000
NODE_ENV=development

# Upload Configuration
MAX_FILE_SIZE=10485760
ALLOWED_MIME_TYPES=application/pdf,image/png,image/jpeg
STORAGE_LOCATION=./invoices

# Logging Configuration
LOG_LEVEL=info
LOG_ENABLE_CONSOLE=true
LOG_ENABLE_FILE=true